{
  "name": "canvas-boilerplate",
  "version": "0.0.1",
  "description": "Simple canvas playground boilerplate",
  "author": "J Lopes <mail@jlopes.eu>",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --hot --inline",
    "build": "webpack --mode production",
    "test": "jest"
  },
  "keywords": [
    "webpack",
    "js",
    "canvas"
  ],
  "devDependencies": {
    "@babel/core": "^7.0.0-rc.0",
    "@babel/plugin-transform-react-jsx": "^7.0.0-rc.0",
    "@babel/plugin-transform-spread": "^7.0.0-rc.0",
    "@babel/preset-env": "^7.0.0-rc.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.0-beta.4",
    "clipboardy": "^1.2.3",
    "css-loader": "^0.28.11",
    "css-mqpacker": "^6.0.1",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "postcss-color-function": "^4.0.1",
    "postcss-custom-media": "^7.0.7",
    "postcss-extend": "^1.0.5",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.6",
    "postcss-preset-env": "^6.5.0",
    "preact": "^8.3.0",
    "style-loader": "^0.19.0",
    "terser-webpack-plugin": "^1.2.1",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14",
    "webpackbar": "^3.1.4"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "shippedProposals": true
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-transform-spread",
      [
        "@babel/plugin-transform-react-jsx",
        {
          "pragma": "h"
        }
      ]
    ]
  },
  "postCSS": {
    "parser": "sugarss",
    "plugins": {
      "autoprefixer": {
        "browsers": [
          "last 3 versions"
        ]
      },
      "postcss-import": {},
      "postcss-cssnext": {
        "browsers": [
          "last 3 versions"
        ]
      },
      "cssnano": {}
    }
  },
  "jest": {
    "testURL": "file:/"
  }
}
